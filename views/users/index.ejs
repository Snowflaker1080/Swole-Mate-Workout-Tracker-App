<h1>All Users</h1>
<ul>
  <% users.forEach(user => { %>
    <li>
      <%= user.username %> - 
      <a href="/users/<%= user._id %>">View</a> | 
      <a href="/users/<%= user._id %>/edit">Edit</a> | 
      <form action="/users/<%= user._id %>?_method=DELETE" method="POST" style="display:inline;">
        <button>Delete</button>
      </form>
    </li>
  <% }) %>
</ul>
<a href="/users/new">Create New User</a>